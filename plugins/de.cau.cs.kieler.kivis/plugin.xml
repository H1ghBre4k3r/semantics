<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.0"?>

<plugin>

  <extension point="org.eclipse.emf.ecore.generated_package">
    <package 
       uri = "http://www.cau.de/cs/kieler/KVis" 
       class = "de.cau.cs.kieler.kvis.KVisPackage"
       genModel = "model/generated/KVis.genmodel" /> 
	
  </extension>
  <extension
        point="de.cau.cs.kieler.kicool.processor">
     <Processor
           class="de.cau.cs.kieler.kivis.processor.KiVisJSGenerator">
     </Processor>
     <Processor
           class="de.cau.cs.kieler.kivis.processor.VisualizationGenerator">
     </Processor>
     <Processor
           class="de.cau.cs.kieler.kivis.processor.SimulationVisualizationValues">
     </Processor>
  </extension>
  <extension
        point="de.cau.cs.kieler.kicool.system">
     <TransformationSystem
           system="system/de.cau.cs.kieler.kivis.js.kico">
     </TransformationSystem>
  </extension>




</plugin>
