<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.0"?>

<!--
-->

<plugin>

   <extension point="org.eclipse.ui.newWizards">
      <!-- @generated circuit -->
      <category
            id="org.eclipse.emf.ecore.Wizard.category.ID"
            name="%_UI_Wizard_category"/>
      <wizard
            id="de.cau.cs.kieler.circuit.presentation.CircuitModelWizardID"
            name="%_UI_CircuitModelWizard_label"
            class="de.cau.cs.kieler.circuit.presentation.CircuitModelWizard"
            category="org.eclipse.emf.ecore.Wizard.category.ID"
            icon="icons/full/obj16/CircuitModelFile.gif">
         <description>%_UI_CircuitModelWizard_description</description>
         <selection class="org.eclipse.core.resources.IResource"/>
      </wizard>
   </extension>

   <extension point="org.eclipse.ui.editors">
      <!-- @generated circuit -->
      <editor
            id="de.cau.cs.kieler.circuit.presentation.CircuitEditorID"
            name="%_UI_CircuitEditor_label"
            icon="icons/full/obj16/CircuitModelFile.gif"
            extensions="circuit"
            class="de.cau.cs.kieler.circuit.presentation.CircuitEditor"
            contributorClass="de.cau.cs.kieler.circuit.presentation.CircuitActionBarContributor">
      </editor>
   </extension>

</plugin>
