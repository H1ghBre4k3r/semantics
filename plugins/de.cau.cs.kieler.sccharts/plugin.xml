<plugin>

   <extension point="org.eclipse.emf.ecore.generated_package">
      <package
            uri="http://kieler.cs.cau.de/sccharts/0.3.0"
            class="de.cau.cs.kieler.sccharts.SCChartsPackage"
            genModel="model/sccharts.genmodel"/>
   </extension>
   
   <extension-point id="text.generator.extension" 
        name="SCT Generator Extension" 
        schema="schema/de.cau.cs.kieler.sccharts.text.generator.extension.exsd"/>
   <extension
         point="de.cau.cs.kieler.sccharts.text.generator.extension">
      <SCTGeneratorExtension
            class="de.cau.cs.kieler.sccharts.text.sctgenerator.extensions.AbortExtension">
      </SCTGeneratorExtension>
      <SCTGeneratorExtension
            class="de.cau.cs.kieler.sccharts.text.sctgenerator.extensions.ActionExtension">
      </SCTGeneratorExtension>
   </extension>   

     <extension
           point="de.cau.cs.kieler.kicool.resource.extension">
        <ExtensionInformation
              ePackageName="sccharts"
              editor_id="de.cau.cs.kieler.sccharts.text.SCTX"
              extensionName="sctx"
              isXMI="true">
        </ExtensionInformation>
     </extension>
     <extension
           point="de.cau.cs.kieler.kicool.processor">
        <Processor
              class="de.cau.cs.kieler.sccharts.processors.transformators.SurfaceDepth">
        </Processor>
        <Processor
              class="de.cau.cs.kieler.sccharts.processors.transformators.TriggerEffect">
        </Processor>
        <Processor
              class="de.cau.cs.kieler.sccharts.processors.transformators.Abort">
        </Processor>
        <Processor
              class="de.cau.cs.kieler.sccharts.processors.transformators.ComplexFinalState">
        </Processor>
        <Processor
              class="de.cau.cs.kieler.sccharts.processors.transformators.Connector">
        </Processor>
        <Processor
              class="de.cau.cs.kieler.sccharts.processors.transformators.WeakSuspend">
        </Processor>
        <Processor
              class="de.cau.cs.kieler.sccharts.processors.transformators.Suspend">
        </Processor>
        <Processor
              class="de.cau.cs.kieler.sccharts.processors.transformators.Static">
        </Processor>
        <Processor
              class="de.cau.cs.kieler.sccharts.processors.transformators.Signal">
        </Processor>
        <Processor
              class="de.cau.cs.kieler.sccharts.processors.transformators.Reference">
        </Processor>
        <Processor
              class="de.cau.cs.kieler.sccharts.processors.transformators.Pre">
        </Processor>
        <Processor
              class="de.cau.cs.kieler.sccharts.processors.transformators.Map">
        </Processor>
        <Processor
              class="de.cau.cs.kieler.sccharts.processors.transformators.Initialization">
        </Processor>
        <Processor
              class="de.cau.cs.kieler.sccharts.processors.transformators.History">
        </Processor>
        <Processor
              class="de.cau.cs.kieler.sccharts.processors.transformators.For">
        </Processor>
        <Processor
              class="de.cau.cs.kieler.sccharts.processors.transformators.Exit">
        </Processor>
        <Processor
              class="de.cau.cs.kieler.sccharts.processors.transformators.Entry">
        </Processor>
        <Processor
              class="de.cau.cs.kieler.sccharts.processors.transformators.During">
        </Processor>
        <Processor
              class="de.cau.cs.kieler.sccharts.processors.transformators.Deferred">
        </Processor>
        <Processor
              class="de.cau.cs.kieler.sccharts.processors.transformators.CountDelay">
        </Processor>
        <Processor
              class="de.cau.cs.kieler.sccharts.processors.transformators.Const">
        </Processor>
        <Processor
              class="de.cau.cs.kieler.sccharts.processors.Snapshot">
        </Processor>
        <Processor
              class="de.cau.cs.kieler.sccharts.processors.transformators.Reference">
        </Processor>
        <Processor
              class="de.cau.cs.kieler.sccharts.processors.transformators.For">
        </Processor>
     </extension>
     <extension
           point="de.cau.cs.kieler.kicool.system">
        <TransformationSystem
              system="system/de.cau.cs.kieler.sccharts.core.kico">
        </TransformationSystem>
        <TransformationSystem
              system="system/de.cau.cs.kieler.sccharts.extended.simple.kico">
        </TransformationSystem>
        <TransformationSystem
              system="system/de.cau.cs.kieler.sccharts.extended.kico">
        </TransformationSystem>
        <TransformationSystem
              system="system/de.cau.cs.kieler.sccharts.extended.core.simple.kico">
        </TransformationSystem>
     </extension>


</plugin>
