<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.0"?>

<!--
-->

<plugin>

   <extension point="org.eclipse.emf.ecore.generated_package">
      <!-- @generated scgdep -->
      <package
            uri="http://kieler.cs.cau.de/scgdep/0.1.0"
            class="de.cau.cs.kieler.scgdep.ScgdepPackage"
            genModel="model/scgdep.genmodel"/>
   </extension>

   <extension point="org.eclipse.emf.ecore.generated_package">
      <!-- @generated scg -->
      <package
            uri="http://kieler.cs.cau.de/scg/0.1.0"
            class="de.cau.cs.kieler.scg.ScgPackage"
            genModel="model/scg.genmodel"/>
   </extension>

   <extension point="org.eclipse.emf.ecore.generated_package">
      <!-- @generated scgbb -->
      <package
            uri="http://kieler.cs.cau.de/scgbb/0.1.0"
            class="de.cau.cs.kieler.scgbb.ScgbbPackage"
            genModel="model/scgbb.genmodel"/>
   </extension>
   <extension
         point="org.eclipse.emf.ecore.generated_package">
      <package
            class="de.cau.cs.kieler.scgsched.ScgschedPackage"
            genModel="model/scgsched.genmodel"
            uri="http://kieler.cs.cau.de/scgsched/0.1.0">
      </package>
   </extension>
   <extension
         point="org.eclipse.ui.handlers">
      <handler
            class="de.cau.cs.kieler.scg.handlers.SCGraphDependencyModelFileHandler"
            commandId="de.cau.cs.kieler.scg.commands.SCGToSCGDEPTransformation">
      </handler>
      <handler
            class="de.cau.cs.kieler.scg.handlers.SCGraphBasicBlockModelFileHandler"
            commandId="de.cau.cs.kieler.scg.commands.SCGDEPToSCGBBTransformation">
      </handler>
      <handler
            class="de.cau.cs.kieler.scg.handlers.SCGraphSchedulingModelFileHandler"
            commandId="de.cau.cs.kieler.scg.commands.SCGBBTToSCGSCHEDSCGraphTransformation">
      </handler>
      <handler
            class="de.cau.cs.kieler.scg.handlers.SequentialSCGraphModelFileHandler"
            commandId="de.cau.cs.kieler.scg.commands.SCGSchedToSeqSCGTransformation">
      </handler>
      <handler
            class="de.cau.cs.kieler.scg.handlers.CompleteSCGraphModelFileHandler"
            commandId="de.cau.cs.kieler.scg.commands.AllSCGTransformations">
      </handler>
   </extension>
   <extension
         point="org.eclipse.ui.commands">
      <category
            id="de.cau.cs.kieler.scg.commands.category"
            name="SCG Category">
      </category>
      <command
            categoryId="de.cau.cs.kieler.scg.commands.category"
            id="de.cau.cs.kieler.scg.commands.SCGToSCGDEPTransformation"
            name="SCG with Dependency Data">
      </command>
      <command
            categoryId="de.cau.cs.kieler.scg.commands.category"
            id="de.cau.cs.kieler.scg.commands.SCGDEPToSCGBBTransformation"
            name="SCG with Basic Block Data">
      </command>
      <command
            categoryId="de.cau.cs.kieler.scg.commands.category"
            id="de.cau.cs.kieler.scg.commands.SCGBBTToSCGSCHEDSCGraphTransformation"
            name="SCG with Scheduling Data">
      </command>
      <command
            categoryId="de.cau.cs.kieler.scg.commands.category"
            id="de.cau.cs.kieler.scg.commands.SCGSchedToSeqSCGTransformation"
            name="Sequentialized SCG">
      </command>
      <command
            id="de.cau.cs.kieler.scg.commands.AllSCGTransformations"
            name="All SCG Transformations">
      </command>
   </extension>
   <extension
         point="org.eclipse.ui.menus">
      <menuContribution
            locationURI="popup:org.eclipse.ui.popup.any?after=additions">
         <menu
               id="de.cau.cs.kieler.scg.scgtransformations"
               label="Transform SCG"
               mnemonic="T">
            <separator
                  name="de.cau.cs.kieler.core.ui.separator1"
                  visible="true">
            </separator>
         </menu>
      </menuContribution>
      <menuContribution
            locationURI="popup:de.cau.cs.kieler.scg.scgtransformations">
         <command
               commandId="de.cau.cs.kieler.scg.commands.AllSCGTransformations"
               id="de.cau.kieler.scg.menus.AllSCGTransformations"
               label="All SCG Transformations"
               mnemonic="A"
               style="push">
            <visibleWhen
                  checkEnabled="true">
               <iterate
                     ifEmpty="false"
                     operator="or">
                  <adapt
                        type="org.eclipse.core.resources.IFile">
                     <test
                           forcePluginActivation="true"
                           property="org.eclipse.core.resources.extension"
                           value="scg">
                     </test>
                  </adapt>
               </iterate>
            </visibleWhen>
         </command>
      </menuContribution>
      <menuContribution
            locationURI="popup:de.cau.cs.kieler.scg.scgtransformations">
         <command
               commandId="de.cau.cs.kieler.scg.commands.AllSCGTransformations"
               label="All SCG Transformations"
               mnemonic="A"
               style="push">
            <visibleWhen
                  checkEnabled="true">
               <iterate
                     ifEmpty="false"
                     operator="or">
                  <adapt
                        type="org.eclipse.core.resources.IFile">
                     <test
                           forcePluginActivation="true"
                           property="org.eclipse.core.resources.extension"
                           value="scgdep">
                     </test>
                  </adapt>
               </iterate>
            </visibleWhen>
         </command>
      </menuContribution>
      <menuContribution
            locationURI="popup:de.cau.cs.kieler.scg.scgtransformations">
         <command
               commandId="de.cau.cs.kieler.scg.commands.AllSCGTransformations"
               label="All SCG Transformations"
               mnemonic="A"
               style="push">
            <visibleWhen
                  checkEnabled="true">
               <iterate
                     ifEmpty="false"
                     operator="or">
                  <adapt
                        type="org.eclipse.core.resources.IFile">
                     <test
                           forcePluginActivation="true"
                           property="org.eclipse.core.resources.extension"
                           value="scgbb">
                     </test>
                  </adapt>
               </iterate>
            </visibleWhen>
         </command>
      </menuContribution>
      <menuContribution
            locationURI="popup:de.cau.cs.kieler.scg.scgtransformations">
         <separator
               name="de.cau.cs.kieler.scg.separator1"
               visible="true">
         </separator>
      </menuContribution>
      <menuContribution
            locationURI="popup:de.cau.cs.kieler.scg.scgtransformations">
         <command
               commandId="de.cau.cs.kieler.scg.commands.SCGToSCGDEPTransformation"
               label="SCG with Dependency Data"
               mnemonic="A"
               style="push">
            <visibleWhen
                  checkEnabled="true">
               <iterate
                     ifEmpty="false"
                     operator="and">
                  <adapt
                        type="org.eclipse.core.resources.IFile">
                     <test
                           forcePluginActivation="true"
                           property="org.eclipse.core.resources.extension"
                           value="scg">
                     </test>
                  </adapt>
               </iterate>
            </visibleWhen>
         </command>
      </menuContribution>
      <menuContribution
            locationURI="popup:de.cau.cs.kieler.scg.scgtransformations">
         <command
               commandId="de.cau.cs.kieler.scg.commands.SCGDEPToSCGBBTransformation"
               label="SCG with Basic Block Data"
               mnemonic="A"
               style="push">
            <visibleWhen
                  checkEnabled="true">
               <iterate
                     ifEmpty="false"
                     operator="and">
                  <adapt
                        type="org.eclipse.core.resources.IFile">
                     <test
                           forcePluginActivation="true"
                           property="org.eclipse.core.resources.extension"
                           value="scgdep">
                     </test>
                  </adapt>
               </iterate>
            </visibleWhen>
         </command>
      </menuContribution>
      <menuContribution
            locationURI="popup:de.cau.cs.kieler.scg.scgtransformations">
         <command
               commandId="de.cau.cs.kieler.scg.commands.SCGBBTToSCGSCHEDSCGraphTransformation"
               label="SCG with Scheduling Data"
               mnemonic="A"
               style="push">
            <visibleWhen
                  checkEnabled="true">
               <iterate
                     ifEmpty="false"
                     operator="and">
                  <adapt
                        type="org.eclipse.core.resources.IFile">
                     <test
                           forcePluginActivation="true"
                           property="org.eclipse.core.resources.extension"
                           value="scgbb">
                     </test>
                  </adapt>
               </iterate>
            </visibleWhen>
         </command>
      </menuContribution>
      <menuContribution
            locationURI="popup:de.cau.cs.kieler.scg.scgtransformations">
         <command
               commandId="de.cau.cs.kieler.scg.commands.SCGSchedToSeqSCGTransformation"
               label="Sequentialized SCG"
               mnemonic="A"
               style="push">
            <visibleWhen
                  checkEnabled="true">
               <iterate
                     ifEmpty="false"
                     operator="and">
                  <adapt
                        type="org.eclipse.core.resources.IFile">
                     <test
                           forcePluginActivation="true"
                           property="org.eclipse.core.resources.extension"
                           value="scgsched">
                     </test>
                  </adapt>
               </iterate>
            </visibleWhen>
         </command>
      </menuContribution>
   </extension>


</plugin>
