<plugin>

   <extension point="org.eclipse.ui.newWizards">
      <category
            id="de.cau.cs.kieler.sccharts"
            name="KIELER SCCharts"/>
      <wizard
            id="de.cau.cs.kieler.sccharts.presentation.SCChartsModelWizardID"
            name="SCCharts Model"
            class="de.cau.cs.kieler.sccharts.presentation.SCChartsModelWizard"
            category="de.cau.cs.kieler.sccharts"
            icon="icons/full/obj16/SCChartsModelFile.gif">
         <description>KIELER SCCharts Models.</description>
         <selection class="org.eclipse.core.resources.IResource"/>
      </wizard>
   </extension>

null
   <extension point="org.eclipse.ui.editors">
      <editor
            id="de.cau.cs.kieler.sccharts.presentation.SCChartsEditorID"
            name="KIELER SCCharts Editor"
            icon="icons/full/obj16/SCChartsModelFile.gif"
            extensions="scc"
            class="de.cau.cs.kieler.sccharts.presentation.SCChartsEditor"
            contributorClass="de.cau.cs.kieler.sccharts.presentation.SCChartsActionBarContributor">
      </editor>
   </extension>

   <extension
         point="de.cau.cs.kieler.kico.ui.transformation">
      <editor
            editor="de.cau.cs.kieler.sccharts.presentation.SCChartsEditorID"
            features="HISTORY, WEAKSUSPEND,DEFERRED,STATIC,SIGNAL,COUNTDELAY,PRE,SUSPEND,COMPLEXFINALSTATE,ABORT,ABORTDEFAULT,ABORTALTERNATIVE,DURING,INITIALIZATION,ENTRY,EXIT,CONNECTOR,SCADEQUARTZESTEREL, STATECHARTS, SYNCCHARTS, EXTENDED, CORE, TRIGGEREFFECT, SURFACEDEPTH, SCGRAPH, SCG, SCGDEP, SCGBB, SCGBBSCPLUS, SCGSCHEDSIMPLE, SCGSCHEDCLUSTER, SCGSCHEDHYBRID, SCGSEQ, SCGBBGROUP, SCGSCHEDGROUP, MAP, FOR, REFERENCE, CONST, EXPANSIONS,CODEGENERATION, SCG2C">
      </editor>
   </extension>

null
</plugin>
