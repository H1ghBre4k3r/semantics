<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
   <extension
         point="de.cau.cs.kieler.kico.transformation">
      <transformationMethod
            class="de.cau.cs.kieler.scg.s.transformations.S2C"
            id="S2C"
            method="transform"
            name="C Code">
      </transformationMethod>
      <transformationMethod
            class="de.cau.cs.kieler.scg.s.transformations.S2Java"
            id="S2JAVA"
            method="transform"
            name="Java Code">
      </transformationMethod>
      <transformationMethod
            class="de.cau.cs.kieler.scg.s.transformations.SCG2S"
            dependencies="S2TARGET"
            id="SCG2S"
            method="transform"
            name="S Code">
      </transformationMethod>
      <transformationGroup
            alternatives="true"
            id="S2TARGET"
            name="Target"
            transformations="S2C, S2JAVA">
      </transformationGroup>
      <transformationGroup
            alternatives="false"
            id="CODEGENERATION"
            name="Code Generation"
            transformations="SCG2S, S2TARGET">
      </transformationGroup>
   </extension>
   <extension
         point="de.cau.cs.kieler.kico.extension">
      <ExtensionInformation
            className="Program"
            editor_id="de.cau.cs.kieler.s.S"
            extensionName="s"
            isXMI="false">
      </ExtensionInformation>
   </extension>

</plugin>
