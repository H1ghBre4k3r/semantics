<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.0"?>

<!--
-->

<plugin>

   <extension
         point="de.cau.cs.kieler.kico.transformation">
      <transformationMethod
            class="de.cau.cs.kieler.scgprios.transform.NodePrioritiesTransformation"
            dependencies="SCGOPTNODEPRIOS"
            id="SCGNODEPRIOS"
            method="transform"
            name="NodePriorities">
      </transformationMethod>
      <transformationMethod
            class="de.cau.cs.kieler.scgprios.transform.OptimizedNodePriortiesTransformation"
            dependencies="SCGOPTTSIDS"
            id="SCGOPTNODEPRIOS"
            method="transform"
            name="OptNodePriorities">
      </transformationMethod>
      <transformationMethod
            class="de.cau.cs.kieler.scgprios.transform.ThreadSegmentIDsTransformation"
            dependencies="SCGOPTTSIDS"
            id="SCGTSIDS"
            method="transform"
            name="ThreadSegmentIDs">
      </transformationMethod>
      <transformationMethod
            class="de.cau.cs.kieler.scgprios.transform.SCGDEPtoSCGPRIOTransformation"
            dependencies="SCLP"
            id="SCGPRIO"
            method="transform"
            name="Priorities">
      </transformationMethod>
      <transformationMethod
            class="de.cau.cs.kieler.scgprios.transform.OptimizedThreadSegmentIDsTransformation"
            dependencies="SCGOPTPRIOIDS"
            id="SCGOPTTSIDS"
            method="transform"
            name="OptThreadSegmentIDs">
      </transformationMethod>
      <transformationMethod
            class="de.cau.cs.kieler.scgprios.transform.OptimizedPrioIDsTransformation"
            dependencies="SCLP"
            id="SCGOPTPRIOIDS"
            method="transform"
            name="OptPrioIDs">
      </transformationMethod>
      <transformationMethod
            class="de.cau.cs.kieler.scgprios.transform.OptimizeSCGTransformation"
            dependencies="SCGNODEPRIOS, SCGPRIO"
            id="OPTSCG"
            method="transform"
            name="OptimizeSCG">
      </transformationMethod>
   </extension>

   <extension point="org.eclipse.emf.ecore.generated_package">
      <!-- @generated scgprio -->
      <package
            uri="http://kieler.cs.cau.de/scgprio/0.1.0"
            class="scgprio.ScgprioPackage"
            genModel="model/scgprio.genmodel"/>
   </extension>

   <extension point="org.eclipse.emf.ecore.generated_package">
      <!-- @generated scgprios -->
      <package
            uri="http://kieler.cs.cau.de/scgprios/0.1.0"
            class="de.cau.cs.kieler.scgprios.ScgpriosPackage"
            genModel="model/scgprios.genmodel"/>
   </extension>

</plugin>
