<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
   <extension
         point="de.cau.cs.kieler.sim.kiem.json.datacomponent">
      <content
            class="de.cau.cs.kieler.sccharts.sim.sclp.SCChartsSCLPDataComponent"
            id="de.cau.cs.kieler.sccharts.sim.sclp"
            name="SCCharts / SCG Simulator (SCLP)">
      </content>
   </extension>
   <extension
         point="de.cau.cs.kieler.sim.kiem.config.DefaultScheduleContributor">
      <executionFile
            file="execution/sccharts_sclp.execution">
         <editor
               id="de.cau.cs.kieler.sccharts.text.sct.Sct"
               name="Textual SCCharts (SCT) Editor"
               priority="100">
         </editor>
         <editor
               id="de.cau.cs.kieler.scg.presentation.ScgEditorID"
               name="SCG (Tree) Editor"
               priority="100">
         </editor>
      </executionFile>
      <executionFile
            file="execution/sccharts_sclp_no_visualization.execution">
         <editor
               id="de.cau.cs.kieler.sccharts.text.sct.Sct"
               name="Textual SCCharts (SCT) Editor"
               priority="99">
         </editor>
         <editor
               id="de.cau.cs.kieler.scg.presentation.ScgEditorID"
               name="SCG (Tree) Editor"
               priority="101">
         </editor>
      </executionFile>
   </extension>
   <extension
         point="de.cau.cs.kieler.kico.transformation">
      <transformationMethod
            class="de.cau.cs.kieler.sccharts.sim.sclp.xtend.SimulationVisualization"
            dependencies="DURING"
            id="SCCHARTS_SCLP_SIMULATION_VISUALIZATION"
            method="transformVisualization"
            name="Visualization">
      </transformationMethod>
      <transformationGroup
            alternatives="true"
            id="SCCHARTS_SCLP_SIMULATION"
            isCategory="false"
            isFeatureGroup="false"
            name="SCCharts Simulation SCLP"
            transformations="SCCHARTS_SCLP_SIMULATION_NO_VISUALIZATION, SCCHARTS_SCLP_SIMULATION_VISUALIZATION">
      </transformationGroup>
      <transformationMethod
            class="de.cau.cs.kieler.sccharts.sim.sclp.xtend.SimulationVisualization"
            id="SCCHARTS_SCLP_SIMULATION_NO_VISUALIZATION"
            method="transformNoVisualization"
            name="No Visualization">
      </transformationMethod>
   </extension>

</plugin>
