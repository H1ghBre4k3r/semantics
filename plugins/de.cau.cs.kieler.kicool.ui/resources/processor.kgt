                pos: x = 24 y = 0
                krectangle {
                    styles:
                        foreground = 200r,200g,200b,0a 
                        background = 0r,255g,0b,255a -> 0r,128g,0b,255a (45)
                }
            }
            knode seperator {
                size: width = 1.5 height = 3
