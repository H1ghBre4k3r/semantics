<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>

   <extension
         point="org.eclipse.ui.views">
      <category
            name="KIELER Comparison"
            id="de.cau.cs.kieler.comparison.ui">
      </category>
      <view
            name="Comparison Data View"
            icon="icons/sample.gif"
            category="de.cau.cs.kieler.comparison.ui"
            class="de.cau.cs.kieler.comparison.ui.views.DataView"
            id="de.cau.cs.kieler.comparison.ui.views.DataView">
      </view>
   </extension>
   <extension
         point="org.eclipse.ui.perspectiveExtensions">
      <perspectiveExtension
            targetID="org.eclipse.jdt.ui.JavaPerspective">
         <view
               ratio="0.5"
               relative="org.eclipse.ui.views.ProblemView"
               relationship="right"
               id="de.cau.cs.kieler.comparison.ui.views.DataView">
         </view>
      </perspectiveExtension>
   </extension>
   <extension
         point="org.eclipse.help.contexts">
      <contexts
            file="contexts.xml">
      </contexts>
   </extension>

</plugin>
