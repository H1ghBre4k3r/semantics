<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.0"?>

<plugin>
   <extension point="org.eclipse.emf.ecore.generated_package">
      <!-- @generated SCL -->
      <package
            uri="http://kieler.cs.cau.de/scl/scl"
            class="de.cau.cs.kieler.scl.scl.SclPackage"
            genModel="model/SCL.genmodel"/>
   </extension>
   <extension
         point="de.cau.cs.kieler.kico.extension">
      <ExtensionInformation
            className="SCLProgram"
            editor_id="de.cau.cs.kieler.scl.SCL"
            extensionName="scl"
            isXMI="false">
      </ExtensionInformation>
   </extension>
   <extension
         point="de.cau.cs.kieler.kico.feature">
      <featureClass
            class="de.cau.cs.kieler.scl.features.SCL">
      </featureClass>
   </extension>
   <extension
         point="de.cau.cs.kieler.kico.transformation">
      <productionTransformationClass
            class="de.cau.cs.kieler.scl.transformations.SCLToSCGTransformation">
      </productionTransformationClass>
   </extension>

</plugin>
