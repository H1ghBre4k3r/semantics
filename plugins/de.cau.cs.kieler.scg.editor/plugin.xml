<plugin>

   <extension point="org.eclipse.ui.newWizards">
      <!-- @generated scg -->
      <category
            id="org.eclipse.emf.ecore.Wizard.category.ID"
            name="%_UI_Wizard_category"/>
      <wizard
            id="de.cau.cs.kieler.scg.presentation.ScgModelWizardID"
            name="%_UI_ScgModelWizard_label"
            class="de.cau.cs.kieler.scg.presentation.ScgModelWizard"
            category="org.eclipse.emf.ecore.Wizard.category.ID"
            icon="icons/full/obj16/ScgModelFile.gif">
         <description>%_UI_ScgModelWizard_description</description>
         <selection class="org.eclipse.core.resources.IResource"/>
      </wizard>
   </extension>



   <extension point="org.eclipse.ui.editors">
      <!-- @generated scg -->
      <editor
            id="de.cau.cs.kieler.scg.presentation.ScgEditorID"
            name="%_UI_ScgEditor_label"
            icon="icons/full/obj16/ScgModelFile.gif"
            extensions="scg"
            class="de.cau.cs.kieler.scg.presentation.ScgEditor"
            contributorClass="de.cau.cs.kieler.scg.presentation.ScgActionBarContributor">
      </editor>
   </extension>

   <extension
         point="de.cau.cs.kieler.kico.ui.transformation">
      <editor
            editor="de.cau.cs.kieler.scg.presentation.ScgEditorID"
            label="Netlist Compilation"
            priority="100"
            features="scg, scg.dependency, scg.basicblock, scg.guard, scg.scheduling, scg.sequentialize, codegeneration, s, s.target"
            preferred="scg.basicblock.sc, s.c"></editor>
   </extension>
   <extension
         point="de.cau.cs.kieler.klighd.ui.view.editor">
      <editor
            controllerID="de.cau.cs.kieler.klighd.ui.view.controllers.EcoreXtextSaveUpdateController"
            editorID="de.cau.cs.kieler.scg.presentation.ScgEditorID">
      </editor>
   </extension>

</plugin>
