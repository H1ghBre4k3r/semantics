/* KEXT Test File for arithmetic operations
 * Addition specific operations
 * 
 * @author ssm
 * @date 2015-06-10
 */

int z, x = 1, y = 3;

@check "arithmetics.shiftleft.literals"
z = 5 << 5

@check "arithmetics.shiftleft.literal.VO"
z = 1 << y

@check "arithmetics.shiftleft.VO.literals"
z = x << 2

@check "arithmetics.shiftleft.VOs"
z = x << y


@check "arithmetics.shiftright.literals"
z = 5 >> 5

@check "arithmetics.shiftright.literal.VO"
z = 1 >> y

@check "arithmetics.shiftright.VO.literals"
z = x >> 2

@check "arithmetics.shiftright.VOs"
z = x >> y


@check "arithmetics.shiftunsignedright.literals"
z = 5 >>> 5

@check "arithmetics.shiftunsignedright.literal.VO"
z = 1 >>> y

@check "arithmetics.shiftunsignedright.VO.literals"
z = x >>> 2

@check "arithmetics.shiftunsignedright.VOs"
z = x >>> y
