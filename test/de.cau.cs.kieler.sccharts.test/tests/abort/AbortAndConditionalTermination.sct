@Transformation ABORT
scchart AbortAndConditionalTermination {
  input int I1;
  input int I2;
  output int O;
  
  initial state A {
    
    initial state A0
    --> A1;
    
    final state A1;
  }
  --> F1 with I1 / O = 0
  >-> F2 immediate with I2 / O = 1;
  
  final state F1;
  final state F2;
}