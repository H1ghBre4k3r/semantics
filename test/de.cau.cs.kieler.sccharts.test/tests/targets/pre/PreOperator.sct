
@Transformation PRE
scchart PreOperator {
  int x = 1;
  int y = 0;
  int _pre_x = 1;
  int _cur_x = 1;

  initial state A
  --> B / x = x + 1;

  state B
  --> A / y = _pre_x;

  region _Pre:

  initial state _Init
  --> _Wait immediate / _cur_x = x;

  final state _Wait
  --> _Init / _pre_x = _cur_x;
}