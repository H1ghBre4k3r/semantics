
@Transformation CONNECTOR
scchart ConnectorState {
  input bool I;
  input output bool O;

  initial state A
  --> C with I / O = true
  --> B;

  state B
  --> C immediate with !I / O = false;

  state C
  --> F1 immediate with I / O = true
  --> F2 immediate with !O / O = true;

  final state F1;

  final state F2;
}