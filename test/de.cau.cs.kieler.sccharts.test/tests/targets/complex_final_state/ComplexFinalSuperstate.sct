
@Transformation COMPLEXFINALSTATE
scchart ComplexFinalSuperstate {
  input bool I1;
  input bool I2;
  output bool O;

  initial state A {
    bool _abort = false;
    bool _term = false;

    initial state A0
    --> A1 with I1 / _term = true;

    state A1 {

      initial state A1_1
      --> A1_2 / O = true;

      final state A1_2;
    }
    --> A0 with I2 / _term = false
    --> _Final immediate with _abort;

    final state _Final;

    region _Watch:

    initial state _Watch
    --> _Aborted immediate with _term / _abort = true;

    final state _Aborted;
  }
  >-> F;

  final state F;
}