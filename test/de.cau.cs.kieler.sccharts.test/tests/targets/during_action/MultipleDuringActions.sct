
@Transformation DURING
scchart DuringAction {
  input bool I;
  output bool O;
  int x = 0;

  initial state A {

    initial state A0
    --> A1;

    state A1 {

      region _During:

      initial state _I
      --> _I with / I = !I;
    }
    --> A0;

    region _During:

    initial state _I
    --> _I with / O = !O;

    region _During2:

    initial state _I
    --> _I with I / x = x + 1;

    region _During3:

    initial state _I
    --> _I with !I / x = x - 1;
  };
}