
@Transformation ENTRY
scchart MultipleEntryActions {
  output bool O;

  initial state A {

    region _Entry:

    initial state _Init
    --> _C immediate with / O = true;

    final state _Done;

    connector state _C
    --> _Done immediate with / O = false;
  }
  >-> _Exit;

  final state F;

  state _Exit
  --> F;
}