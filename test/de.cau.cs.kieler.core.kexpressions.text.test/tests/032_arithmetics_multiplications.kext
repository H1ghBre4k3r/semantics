int z, x = 1, y = 3;

@check arithmetics.multiplication.literals
z = 5 * 5

@check arithmetics.multiplication.literal.VO
z = 1 * y

@check arithmetics.multiplication.VO.literals
z = x * 2

@check arithmetics.multiplication.VOs
z = x * y

@check arithmetics.multiplication.literals.distorted#001
@expect "z = 5 * 5"
z = 5* 5

@check arithmetics.multiplication.literals.distorted#002
@expect "z = 5 * 5"
z = 5*5

@check arithmetics.multiplication.literals.distorted#003
@expect "z = 5 * 5"
z = 5 *5

@check arithmetics.multiplication.literal.VO.distorted#001
@expect "z = 1 * y"
z = 1* y

@check arithmetics.multiplication.literal.VO.distorted#002
@expect "z = 1 * y"
z = 1*y

@check arithmetics.multiplication.literal.VO.distorted#003
@expect "z = 1 * y"
z = 1 *y

@check arithmetics.multiplication.VO.literals.distorted#001
@expect "z = x * 2"
z = x* 2

@check arithmetics.multiplication.VO.literals.distorted#002
@expect "z = x * 2"
z = x*2

@check arithmetics.multiplication.VO.literals.distorted#003
@expect "z = x * 2"
z = x *2

@check arithmetics.multiplication.VOs.distorted#001
@expect "z = x * y"
z = x* y

@check arithmetics.multiplication.VOs.distorted#002
@expect "z = x * y"
z = x*y

@check arithmetics.multiplication.VOs.distorted#003
@expect "z = x * y"
z = x *y