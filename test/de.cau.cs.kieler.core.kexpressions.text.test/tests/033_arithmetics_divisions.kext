int z, x = 1, y = 3;

@check arithmetics.division.literals
z = 5 / 5

@check arithmetics.division.literal.VO
z = 1 / y

@check arithmetics.division.VO.literals
z = x / 2

@check arithmetics.division.VOs
z = x / y

@check arithmetics.division.literals.distorted#001
@expect "z = 5 / 5"
z = 5/ 5

@check arithmetics.division.literals.distorted#002
@expect "z = 5 / 5"
z = 5/5

@check arithmetics.division.literals.distorted#003
@expect "z = 5 / 5"
z = 5 /5

@check arithmetics.division.literal.VO.distorted#001
@expect "z = 1 / y"
z = 1/ y

@check arithmetics.division.literal.VO.distorted#002
@expect "z = 1 / y"
z = 1/y

@check arithmetics.division.literal.VO.distorted#003
@expect "z = 1 / y"
z = 1 /y

@check arithmetics.division.VO.literals.distorted#001
@expect "z = x / 2"
z = x/ 2

@check arithmetics.division.VO.literals.distorted#002
@expect "z = x / 2"
z = x/2

@check arithmetics.division.VO.literals.distorted#003
@expect "z = x / 2"
z = x /2

@check arithmetics.division.VOs.distorted#001
@expect "z = x / y"
z = x/ y

@check arithmetics.division.VOs.distorted#002
@expect "z = x / y"
z = x/y

@check arithmetics.division.VOs.distorted#003
@expect "z = x / y"
z = x /y