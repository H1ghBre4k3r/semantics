<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.0"?>

<plugin>

   <extension point="org.eclipse.emf.ecore.generated_package">
      <package
            uri="http://kieler.cs.cau.de/yakindu/sccharts/"
            class="de.cau.cs.kieler.yakindu.model.sgraph.syncgraph.SyncgraphPackage"
            genModel="model/syncgraph.genmodel"/>
   </extension>
  <!-- Editor -->
   <extension
         point="org.eclipse.ui.editors">
      <editor
            class="de.cau.cs.kieler.yakindu.sccharts.ui.editor.factory.SCCExecutableExtensionFactory:org.yakindu.sct.ui.editor.editor.StatechartDiagramEditor"
            contributorClass="org.yakindu.sct.ui.editor.editor.StatechartDiagramActionbarContributor"
            default="true"
            extensions="scc"
            icon="icons/obj16/logo-16.png"
            id="de.cau.cs.kieler.yakindu.sccharts.ui.editor.SCChartsDiagramEditor"
            matchingStrategy="org.eclipse.gmf.runtime.diagram.ui.resources.editor.parts.DiagramDocumentEditorMatchingStrategy"
            name="KIELER SCCharts Editor">
      </editor>
   </extension>
      
   
   <!-- Wizard -->
   <extension
         point="org.eclipse.ui.newWizards">
     <category id="KIELER" name="KIELER"/>
      <wizard
            category="KIELER"
            class="de.cau.cs.kieler.yakindu.sccharts.ui.editor.factory.SCCExecutableExtensionFactory:org.yakindu.sct.ui.editor.wizards.CreationWizard"
            icon="icons/obj16/SyncCharts-Pictogram-16.png"
            id="de.cau.cs.kieler.yakindu.sccharts.ui.editor.KielerWizard"
            name="KIELER SCCharts Model"
            project="false">
      </wizard>
   </extension>
   
   <!-- Palette Provider -->
    <extension
         point="org.eclipse.gmf.runtime.diagram.ui.paletteProviders">
      <paletteProvider
            class="de.cau.cs.kieler.yakindu.sccharts.ui.editor.factory.SCCExecutableExtensionFactory:org.yakindu.sct.ui.editor.providers.SCTPaletteProvider">
             <Priority
               name="Highest">
         </Priority>
         <editor
               class="org.yakindu.sct.ui.editor.editor.StatechartDiagramEditor">
         </editor>
      </paletteProvider>
  </extension>

	<extension point="org.yakindu.sct.ui.editor.expressions">
		<ExpressionLanguageProvider
			class="de.cau.cs.kieler.yakindu.sccharts.ui.integration.stext.TransitionExpressionProvider"
			resourceExtension="scc"
			semanticTarget="TransitionSpecification">
		</ExpressionLanguageProvider>
		<ExpressionLanguageProvider
			class="de.cau.cs.kieler.yakindu.sccharts.ui.integration.stext.StatechartExpressionProvider"
			resourceExtension="scc"
			semanticTarget="StatechartSpecification">
		</ExpressionLanguageProvider>
	 	<ExpressionLanguageProvider
			class="de.cau.cs.kieler.yakindu.sccharts.ui.integration.stext.StateExpressionProvider"
			resourceExtension="scc"
			semanticTarget="StateSpecification">
			</ExpressionLanguageProvider>
	</extension>
<!-- Property Tabs -->
<extension point="org.eclipse.ui.views.properties.tabbed.propertyContributor" id="prop-contrib">
      <propertyContributor
            contributorId="de.cau.cs.kieler.yakindu.sccharts.ui.editor.SCChartsDiagramEditor"
            labelProvider="org.yakindu.sct.ui.editor.propertysheets.SheetLabelProvider">
         <propertyCategory category="domain"/>
         <propertyCategory category="visual"/>
         <propertyCategory category="extra"/>
      </propertyContributor>
   </extension>
   
    <extension point="org.eclipse.ui.views.properties.tabbed.propertyTabs" id="proptabs">
      <propertyTabs contributorId="de.cau.cs.kieler.yakindu.sccharts.ui.editor.SCChartsDiagramEditor">
          <propertyTab
             category="domain"
             id="property.tab.domain"
             label="Model"/>
          <propertyTab
             category="visual"
             id="property.tab.AppearancePropertySection"
             label="Appearance"/>
          <propertyTab
             category="visual"
             id="property.tab.DiagramPropertySection"
             label="Diagram"/>
      </propertyTabs>
   </extension>  

<!-- Propertysheets -->
<!-- Property Tabs -->   
   <extension point="org.eclipse.ui.views.properties.tabbed.propertySections" id="propsections">
      <propertySections contributorId="de.cau.cs.kieler.yakindu.sccharts.ui.editor.SCChartsDiagramEditor">
		<!-- State model section -->         
         <propertySection
            id="property.section.domain.state" 
            tab="property.tab.domain"
            class="de.cau.cs.kieler.yakindu.ui.editor.propertysheets.SyncStatePropertySection">
           <input type="de.cau.cs.kieler.yakindu.ui.editor.parts.SyncStateEditPart"/>
           <input type="org.yakindu.sct.ui.editor.editparts.StateNameEditPart"/>
           <input type="org.yakindu.sct.ui.editor.editparts.StateTextCompartmentEditPart"/>
           <input type="org.yakindu.sct.ui.editor.editparts.StateTextCompartmentExpressionEditPart"/>
         </propertySection>
        <!-- Transition model section -->         
         <propertySection
            id="property.section.domain.transition" 
            tab="property.tab.domain"
            class="de.cau.cs.kieler.yakindu.ui.editor.propertysheets.SyncTransitionPropertySection">
            <input type="de.cau.cs.kieler.yakindu.ui.editor.parts.SyncTransitionEditPart"/>
            <input type="org.yakindu.sct.ui.editor.editparts.TransitionExpressionEditPart"/>
         </propertySection>
            <!-- Region model section -->         
         <propertySection
            id="property.section.domain.region" 
            tab="property.tab.domain"
            class="org.yakindu.sct.ui.editor.propertysheets.RegionPropertySection">
            <!--<input type="org.yakindu.sct.ui.editor.editparts.RegionEditPart"/>-->
            <input type="de.cau.cs.kieler.yakindu.ui.editor.parts.SyncRegionEditPart"/>
         </propertySection>
            <!-- Choice model section -->         
         <propertySection
            id="property.section.domain.choice" 
            tab="property.tab.domain"
            class="org.yakindu.sct.ui.editor.propertysheets.ChoicePropertySection">
            <input type="org.yakindu.sct.ui.editor.editparts.ChoiceEditPart"/>
         </propertySection>
         
         <propertySection id="property.section.ConnectorAppearancePropertySection" 
            filter="org.eclipse.gmf.runtime.diagram.ui.properties.filters.ConnectionEditPartPropertySectionFilter" 
            class="org.eclipse.gmf.runtime.diagram.ui.properties.sections.appearance.ConnectionAppearancePropertySection" 
            tab="property.tab.AppearancePropertySection">
         </propertySection>
         <propertySection id="property.section.ShapeColorAndFontPropertySection" 
            filter="org.eclipse.gmf.runtime.diagram.ui.properties.filters.ShapeEditPartPropertySectionFilter" 
            class="org.eclipse.gmf.runtime.diagram.ui.properties.sections.appearance.ShapeColorsAndFontsPropertySection" 
            tab="property.tab.AppearancePropertySection">
         </propertySection> 
         <propertySection id="property.section.DiagramColorsAndFontsPropertySection" 
            filter="org.eclipse.gmf.runtime.diagram.ui.properties.filters.DiagramEditPartPropertySectionFilter" 
            class="org.eclipse.gmf.runtime.diagram.ui.properties.sections.appearance.DiagramColorsAndFontsPropertySection" 
            tab="property.tab.AppearancePropertySection">
         </propertySection>     
         <propertySection id="property.section.RulerGridPropertySection" 
            filter="org.eclipse.gmf.runtime.diagram.ui.properties.filters.DiagramEditPartPropertySectionFilter" 
            class="org.eclipse.gmf.runtime.diagram.ui.properties.sections.grid.RulerGridPropertySection" 
            tab="property.tab.DiagramPropertySection">
         </propertySection>     
 
      </propertySections>
   </extension>
   
   <!-- end Propertysheets -->
 <!-- Modeling Assistant Provider -->
   <extension
         point="org.eclipse.gmf.runtime.emf.ui.modelingAssistantProviders">
      <modelingAssistantProvider
            class="de.cau.cs.kieler.yakindu.sccharts.ui.editor.factory.SCCExecutableExtensionFactory:de.cau.cs.kieler.yakindu.sccharts.ui.editor.assistent.SCCModelingAssistantProvider">
         <Priority
               name="Medium">
         </Priority>
      </modelingAssistantProvider>
   </extension>
   

   
   <!-- EditPart Provider -->
   <extension
         point="org.eclipse.gmf.runtime.diagram.ui.editpartProviders">
      <editpartProvider
            class="de.cau.cs.kieler.yakindu.sccharts.ui.editor.factory.SCCExecutableExtensionFactory:org.yakindu.sct.ui.editor.editor.guice.InjectableEditPartFactory">
         <Priority
               name="High">
         </Priority>
      </editpartProvider>
   </extension>
   
   
    <!-- Layout options -->
    <extension
         point="de.cau.cs.kieler.kiml.layoutInfo">
      <option
            class="org.yakindu.sct.model.sgraph.Region"
            option="de.cau.cs.kieler.diagramType"
            value="de.cau.cs.kieler.layout.diagrams.stateMachine">
      </option>
      <option
            class="de.cau.cs.kieler.yakindu.sgraph.syncgraph.SyncState"
            option="de.cau.cs.kieler.diagramType"
            value="de.cau.cs.kieler.layout.diagrams.boxes">
      </option>
      <option
            class="de.cau.cs.kieler.yakindu.sgraph.syncgraph.SyncState"
            option="de.cau.cs.kieler.spacing"
            value="0.0">
      </option>
      <option
            class="de.cau.cs.kieler.yakindu.sgraph.syncgraph.SyncState"
            option="de.cau.cs.kieler.borderSpacing"
            value="1.0">
      </option>
      <option
            class="de.cau.cs.kieler.yakindu.sgraph.syncgraph.SyncState"
            option="de.cau.cs.kieler.sizeConstraint"
            value="[MINIMUM_SIZE]">
      </option>
      <option
            class="de.cau.cs.kieler.yakindu.sgraph.syncgraph.SyncState"
            option="de.cau.cs.kieler.expandNodes"
            value="true">
      </option>
      <option
            class="de.cau.cs.kieler.yakindu.sgraph.syncgraph.SyncState"
            option="de.cau.cs.kieler.interactive"
            value="true">
      </option>
      <option
            class="org.yakindu.sct.model.sgraph.Region"
            option="de.cau.cs.kieler.borderSpacing"
            value="7.0">
      </option>
      <option
            class="org.yakindu.sct.model.sgraph.Region"
            option="de.cau.cs.kieler.direction"
            value="RIGHT">
      </option>
      <option
            class="de.cau.cs.kieler.yakindu.sgraph.syncgraph.SyncState"
            option="de.cau.cs.kieler.sizeOptions"
            value="DEFAULT_MINIMUM_SIZE">
      </option>
      <semanticOption
            class="org.yakindu.sct.model.sgraph.Region"
            config="de.cau.cs.kieler.yakindu.sccharts.ui.editor.layout.YakinduLayoutConfig">
      </semanticOption>
      <semanticOption
            class="org.yakindu.sct.model.sgraph.State"
            config="de.cau.cs.kieler.yakindu.sccharts.ui.editor.layout.YakinduLayoutConfig">
      </semanticOption>
   </extension>
    <extension
          point="de.cau.cs.kieler.kiml.ui.layoutManagers">
       <manager
             class="de.cau.cs.kieler.yakindu.sccharts.ui.editor.layout.YakinduDiagramLayoutManager"
             priority="30">
       </manager>
    </extension>
</plugin>

	

